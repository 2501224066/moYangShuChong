.navbar {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 32rpx;
  color: #000;
  background: #F7B52C;
  font-size: 36rpx;
  font-weight: 600;
  line-height: 50rpx;
  box-sizing: border-box;
  z-index: 999;
}

.navbar>.icon {
  height: 70rpx;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 20%;
  padding: 0 32rpx;
}

.navbar>.text {
  flex: 1;
  text-align: center;
}

.navbar>.icon>.img {
  width: 50rpx;
  height: 50rpx;
}

.small {
  position: fixed;
  right: 0;
  bottom: 20vh;
  height: 88rpx;
  display: flex;
  align-items: center;
  padding-right: 16rpx;
  background: rgba(245, 245, 245, 0.8);
  border-radius: 198rpx 0rpx 0rpx 198rpx;
  border: 1rpx solid #E5E5E5;
}

.small:hover {
  right: 0;
}

.small>.img {
  width: 48rpx;
  height: 48rpx;
  margin-left: 30rpx;
}

.player {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  background: #F7B52C;
  padding: 14vh 40rpx 10vh 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.player>.title {
  width: 532rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 500;
  color: #fff;
  line-height: 44rpx;
  text-align: center;
}

.player>.audio {
  position: relative;
  width: 508rpx;
  height: 508rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 10rpx solid #efbb62;
  transform: scale(1);
}

.player>.audio>.center {
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  background: #F7B52C;
  border-radius: 50%;
  overflow: hidden;
  left: 154rpx;
  top: 154rpx;
  border: 10rpx solid #efbb62;
  z-index: 1;
}

.player>.line {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 6vh;
}

.player>.line>.time {
  font-size: 20rpx;
  font-weight: 400;
  color: #FFFFFF;
}

.player>.line>.way {
  width: 600rpx;
  height: 8rpx;
  margin: 0 20rpx;
  background: rgba(250, 250, 250, .3);
  border-radius: 4rpx;
  position: relative;
}

.player>.line>.way>.has {
  position: absolute;
  left: 0;
  top: 0;
  height: 8rpx;
  background: #FF671F;
  border-radius: 4rpx;
}

.player>.line>.way>.now {
  position: absolute;
  top: -16rpx;
  margin-left: -12rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: #FFFFFF;
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.2);
}

.player>.line>.way>.now>view {
  position: absolute;
  left: 8rpx;
  top: 8rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #FF671F;
}

.player>.action {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 16rpx;
}

.player>.action>.img {
  width: 80rpx;
  height: 80rpx;
}

.player>.action>.img1 {
  width: 120rpx;
  height: 120rpx;
}

.player>.list {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100vh;
}

.player>.list>.bg {
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.2);
  z-index: 1000;
}

.player>.list>.content {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 26rpx 32rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #243241;
  background: #fff;
  border-radius: 32rpx 32rpx 0 0;
  box-sizing: border-box;
  z-index: 10001;
}

.player>.list>.content>.title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 80rpx;
  color: #333333;
}

.player>.list>.content>.title span {
  font-size: 26rpx;
  color: #999;
  margin-left: 10rpx;
}

.player>.list>.content>.box {
  max-height: 500rpx;
  overflow: auto;
}

.player>.list>.content>.box>.item {
  display: flex;
  align-items: center;
  line-height: 100rpx;
  padding-left: 10rpx;
}

.player>.list>.content>.box>.item>.name {
  margin-left: 20rpx;
  width: 500rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.active {
  color: #F3981C !important;
}

.sharebox {
  position: relative;
  text-align: right;
  left: 320rpx;
}

.button_image {
  height: 50rpx;
  width: 50rpx;
}
