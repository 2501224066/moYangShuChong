.setting {
  padding: 20rpx 32rpx;
}

.setting>.item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  padding: 20rpx 0;
  min-height: 60rpx;
  border-bottom: 1rpx solid #ededed;
}

.setting>.item>.header {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 3rpx solid #ddd;
}

.setting>.item>.img {
  width: 30rpx;
  height: 30rpx;
}
