page {
  background: #F7F9FC;
}

.reserve {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.top {
  flex-shrink: 0;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 32rpx 50rpx 32rpx;
  background: #F3981C;
}

.top>.right {
  display: flex;
  align-items: center;
}

.top>.right>.img {
  width: 36rpx;
  height: 36rpx;
}

.top>.right>.name {
  font-size: 32rpx;
  font-weight: 500;
  color: #FFFFFF;
  margin-right: 10rpx;
}

.week {
  flex-shrink: 0;
  margin-top: -32rpx;
  background: #e9a54c;
  color: #fff;
  font-size: 28rpx;
  line-height: 30rpx;
  padding: 24rpx 40rpx 56rpx 40rpx;
  border-radius: 32rpx 32rpx 0 0;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.week>.num {
  width: 14%;
  padding: 20rpx 0;
  border-radius: 16rpx;
}

.week>.num>view {
  height: 35rpx;
  line-height: 35rpx;
}

.week>.today {
  background: rgba(250, 250, 250, .2);
}

.week>.active {
  background: #fff;
  color: #F3981C;
}

.week>.num .red {
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color: #FF3333;
}

.content {
  flex: 1;
  margin-top: -32rpx;
  border-radius: 32rpx 32rpx 0 0;
  padding: 32rpx 32rpx 50rpx 32rpx;
  overflow: auto;
  background: #F7F9FC;
}

.list>.item {
  margin-bottom: 32rpx;
  background: #fff;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  padding: 32rpx;
}

.list>.item>.up {
  display: flex;
  justify-content: space-between;
}

.list>.item>.up>.img {
  width: 168rpx;
  height: 168rpx;
  border-radius: 16rpx;
  overflow: hidden;
  margin-top: -10rpx;
  background: #fff;
}

.list>.item>.up>.right {
  box-sizing: border-box;
  font-size: 32rpx;
  margin-left: 20rpx;
}

.list>.item>.up>.right>view {
  margin-bottom: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 480rpx;
}

.list>.item>.up>.right span {
  margin: 0 20rpx;
  font-weight: 500;
  color: #333333;
}

.list>.item>.up>.right>.li {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.list>.item>.up>.right>.li>.line {
  flex: 1;
  margin-right: 20rpx;
  height: 16rpx;
  background: #F5F5F5;
  border-radius: 8rpx;
  position: relative;
}

.list>.item>.up>.right>.li>.line>.has {
  position: absolute;
  height: 16rpx;
  background: linear-gradient(270deg, #FAC93C 0%, #F3981C 100%);
  border-radius: 8rpx;
}

.list>.item>.bottom {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.list>.item>.bottom>.btn {
  width: 210rpx;
  text-align: center;
  height: 64rpx;
  border-radius: 44rpx;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 64rpx;
}

.list>.item>.bottom>.btn1 {
  border: 1rpx solid #F3981C;
  color: #F3981C;
}

.list>.item>.bottom>.btn2 {
  border: 1px solid #CCCCCC;
  color: #333333;
}

.list>.item>.bottom>.btn3 {
  width: 88%;
  background: #F5F5F5;
  color: #CCCCCC;
}

.list>.item>.bottom>.btn4 {
  width: 88%;
  background: #F3981C;
  color: #fff;
}

.list>.item>.bottom>.btn5 {
  width: 88%;
  background: #F05B63;
  color: #fff;
}

.list>.item>.bottom>.btn6 {
  background: #F05B63;
  color: #fff;
}

.none {
  margin-top: 10vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}

.none>.img {
  width: 270rpx;
  height: 292rpx;
}



.people{
  width: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  height:100%;
  left: 0;
  top:0;
  z-index: 999;
}

.people>.peoplebox{
  position: absolute;
  border-radius: 10rpx!important;
  left:15%;
  top: 35%;
  background: #fff;
  width:70%;
  border-radius: 30rpx;
  align-items: center;
  justify-content: center;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.2);
}

.title{
  font-size: 40rpx;
  font-weight: 400;
  color: #333333;
  text-align: center;
  margin-bottom: 40rpx;
  margin-top: 40rpx;
}
.bottom-btn{
  display: flex;
  text-align: center;
  font-size: 34rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 0.1px solid #CCCCCC;
  font-weight: 400;
}

.cancelbtn{
  flex: 1;
  border-right: 0.1px solid #CCCCCC;
}

.surebtn{
  flex: 1;
  color: #4dc438;
}

.checkbox {
  text-align: center;
  margin-bottom: 10rpx;
  display: flex;
  padding-left: 60rpx;
  padding-bottom: 40rpx;
  
}

.name{
  margin-left: 20rpx;
  font-size: 40rpx;
  color: #333333;
}